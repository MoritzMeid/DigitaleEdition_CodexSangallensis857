                  <pb xml:id="co_fol_1r" corresp="#co_surf_1r" n="1r"/>
                    <head style="text-align: center">
                        <l><lb facs="#co_line_1r_00" xml:id="co_lb_1r_00" />Der Parcival</l> 
                    </head> 
                    <l n="1"><lb facs="#co_line_1r_01" xml:id="co_lb_1r_01" n="1"/>Ist zwiuel der <choice><abbr>h'zen</abbr><expan>herzen</expan></choice> nahgebur</l>
                    <l n="2"><lb facs="#co_line_1r_02" xml:id="co_lb_1r_02" n="2"/>daz muͦz der sele werden sur</l>
                    <l n="3"><lb facs="#co_line_1r_03" xml:id="co_lb_1r_03" n="3"/>gesmehet unde gezieret.</l>
                    <l n="4"><lb facs="#co_line_1r_04" xml:id="co_lb_1r_04" n="4"/>ist swa sich parrieret.</l>
                    <l n="5"><lb facs="#co_line_1r_05" xml:id="co_lb_1r_05" n="5"/>vn verzaget mannes muͦt.</l>
                    <l n="6"><lb facs="#co_line_1r_06" xml:id="co_lb_1r_06" n="6"/>als agelstern varwe tuͦt.</l>
                    <l n="7"><lb facs="#co_line_1r_07" xml:id="co_lb_1r_07" n="7"/> der mac <choice><sic>dennoc</sic><corr>dennoch</corr></choice> wesen geil.</l>
                    <l n="8"><lb facs="#co_line_1r_08" xml:id="co_lb_1r_08" n="8"/>wand an im sint <choice><sic>eidiv</sic><corr>beidiv</corr></choice> teil.</l>
                    <l n="9"><lb facs="#co_line_1r_09" xml:id="co_lb_1r_09" n="9"/>des himels vnd der helle.</l>
                    <l n="10"><lb facs="#co_line_1r_10" xml:id="co_lb_1r_10" n="10"/>der unstete <choice><sic>geselle</sic><corr>gesell</corr></choice></l>
                    <l n="11"><lb facs="#co_line_1r_11" xml:id="co_lb_1r_11" n="11"/>hat di swarzen <choice><sic>varwe</sic><corr>va</corr></choice> gâr</l>
                    <l n="12"><lb facs="#co_line_1r_12" xml:id="co_lb_1r_12" n="12"/>vnd wirt ouch nah der vinster <choice><sic>varn</sic><corr>var</corr></choice></l>
                    <l n="13"><lb facs="#co_line_1r_13" xml:id="co_lb_1r_13" n="13"/>so habt sich an die blanken.</l>
                    <l n="14"><lb facs="#co_line_1r_14" xml:id="co_lb_1r_14" n="14"/>der mit steten gedanken</l>
                    <l n="15"><lb facs="#co_line_1r_15" xml:id="co_lb_1r_15" n="15"/>Diz fligende bîspel.</l>
                    <l n="16"><lb facs="#co_line_1r_16" xml:id="co_lb_1r_16" n="16"/></l>
                    <l n="17"><lb facs="#co_line_1r_17" xml:id="co_lb_1r_17" n="17"/></l>
